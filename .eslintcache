  }\n      },\n      [webcamRef,setimgSrc]\n    );\n  \n    return (\n        <div className=\"col-lg-12\" style={{margin:\"20px\"}}>\n            <center>\n                <Webcam\n                audio={false}\n                height={400}\n                ref={webcamRef}\n                screenshotFormat=\"image/jpeg\"\n                width={300}\n                videoConstraints={videoConstraints}\n                screenshotQuality={\"1\"}\n                />\n            </center>\n        </div>\n    );\n  };    \n\n  async function storeSnap(exam,image)\n  {\n    await API.post('/proctor/',{\"type\": \"snapshot\",\"exam\":exam,\"image\":image})\n    .then((res) => {\n        console.log('snap stored successfully');\n    })\n    .catch((error) =>\n    {\n        console.log('problem storing snapshot');\n    });\n  }\n\n  export default WebCamCapture;",{"ruleId":"115","replacedBy":"116"},{"ruleId":"117","replacedBy":"118"},{"ruleId":"119","severity":1,"message":"120","line":56,"column":17,"nodeType":"121","endLine":56,"endColumn":60},{"ruleId":"122","severity":1,"message":"123","line":35,"column":6,"nodeType":"124","endLine":35,"endColumn":22,"suggestions":"125"},{"ruleId":"122","severity":1,"message":"126","line":45,"column":6,"nodeType":"124","endLine":45,"endColumn":16,"suggestions":"127"},{"ruleId":"122","severity":1,"message":"128","line":55,"column":6,"nodeType":"124","endLine":55,"endColumn":22,"suggestions":"129"},{"ruleId":"130","severity":1,"message":"131","line":154,"column":34,"nodeType":"132","messageId":"133","endLine":154,"endColumn":36},{"ruleId":"130","severity":1,"message":"131","line":164,"column":34,"nodeType":"132","messageId":"133","endLine":164,"endColumn":36},{"ruleId":"134","severity":1,"message":"135","line":11,"column":12,"nodeType":"136","messageId":"137","endLine":11,"endColumn":18},{"ruleId":"122","severity":1,"message":"138","line":49,"column":7,"nodeType":"124","endLine":49,"endColumn":28,"suggestions":"139"},"no-native-reassign